{
  "name": "paper-helper",
  "displayName": "Paper Helper",
  "description": "Assistant for anyone while reading english papers",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "resource/icon.jpg",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist",
    "package": "npm run clean && npm run compile && vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.1",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^9.0.0",
    "glob": "^9.0.0",
    "mocha": "^8.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "log4js": "^6.9.1",
    "uuid": "^9.0.0",
    "moment": "^2.30.0",
    "vscode-nls-i18n": "0.2.4",
    "vsce": "^1.96.1"
  },
  "contributes": {
    "commands": [
      {
        "command": "PaperHelper.Translate",
        "title": "%PaperHelper.Translate%"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Meiyukichan/paper-helper.git"
  }
}
